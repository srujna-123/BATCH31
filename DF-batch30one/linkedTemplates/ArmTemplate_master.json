{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DF-batch30one"
		},
		"LS_SQL_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SQL'"
		},
		"LS_SQL_PARAM_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SQL_PARAM'"
		},
		"LS_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_blob'"
		},
		"LS_SQL_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserverbatch30one.database.windows.net"
		},
		"LS_SQL_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqlserverbatch31"
		},
		"LS_SQL_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "srujna"
		},
		"LS_SQL_PARAM_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserverbatch31.database.windows.net"
		},
		"LS_SQL_PARAM_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().db_Name}"
		},
		"LS_SQL_PARAM_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "srujna"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DF-batch30one_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_SQL_password": {
						"value": "[parameters('LS_SQL_password')]"
					},
					"LS_SQL_PARAM_password": {
						"value": "[parameters('LS_SQL_PARAM_password')]"
					},
					"LS_blob_connectionString": {
						"value": "[parameters('LS_blob_connectionString')]"
					},
					"LS_SQL_properties_typeProperties_server": {
						"value": "[parameters('LS_SQL_properties_typeProperties_server')]"
					},
					"LS_SQL_properties_typeProperties_database": {
						"value": "[parameters('LS_SQL_properties_typeProperties_database')]"
					},
					"LS_SQL_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQL_properties_typeProperties_userName')]"
					},
					"LS_SQL_PARAM_properties_typeProperties_server": {
						"value": "[parameters('LS_SQL_PARAM_properties_typeProperties_server')]"
					},
					"LS_SQL_PARAM_properties_typeProperties_database": {
						"value": "[parameters('LS_SQL_PARAM_properties_typeProperties_database')]"
					},
					"LS_SQL_PARAM_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQL_PARAM_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}